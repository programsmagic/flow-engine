  "main": "dist/index.js",
  "bin": {
    "flow-engine": "./dist/cli.js",
    "flow": "./dist/cli.js",
    "flow-dev": "./dist/dev.js",
    "flow-monitor": "./dist/monitor.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "dev:server": "node dist/dev.js",
    "monitor": "node dist/monitor.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "prepare": "npm run build",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "demo": "npm run build && npm run dev:server",
    "docs:build": "echo 'Documentation is already built in docs/ directory'",
    "docs:deploy": "./scripts/deploy-docs.sh",
    "docs:serve": "cd docs && python3 -m http.server 8080",
    "docs:preview": "cd docs && open index.html"
  },
